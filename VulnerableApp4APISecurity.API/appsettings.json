{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithProcessName",
      "WithProcessId",
      "WithThreadName",
      "WithThreadId",
      "WithEnvironmentUserName",
      "WithEnvironmentName",
      "WithMemoryUsage"
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Log/Serilog.txt"
        }
      }
    ]
  },
  "ElasticConfiguration": {
    "Uri": "http://elasticsearch:9200"
  },
  "DatabaseSettings": {
    "ConnectionString": "mongodb://mongo:27017/",
    "DatabaseName": "VulnerableApp4APISecurity"
  },
